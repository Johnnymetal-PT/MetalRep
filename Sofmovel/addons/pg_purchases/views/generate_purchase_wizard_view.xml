<odoo>
    <record id="view_generate_purchase_wizard_form" model="ir.ui.view">
        <field name="name">generate.purchase.wizard.form</field>
        <field name="model">generate.purchase.wizard</field>
        <field name="arch" type="xml">
            <form string="Confirmar Criação de Compras">
                <div class="oe_title">
                    <p>Já foram geradas Ordens de Compra para esta Ordem de Venda.</p>
                    <p>Tem a certeza que deseja gerar novamente?</p>
                </div>
                <footer>
                    <button name="confirm_generate_po" string="Sim, Gerar" type="object" class="btn-primary"/>
                    <button string="Cancelar" class="btn-secondary" special="cancel"/>
                </footer>
            </form>
        </field>
    </record>
</odoo>

